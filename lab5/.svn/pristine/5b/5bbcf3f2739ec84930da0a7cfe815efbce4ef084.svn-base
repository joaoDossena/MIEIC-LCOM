#include "video.h"

int set_graphics_mode(uint16_t mode)
{
	reg86_t reg86;

	memset(&reg86, 0, sizeof(reg86));

	reg86.intno = VIDEOCARD_SERVICE;
	reg86.ax = SET_VBE_MODE;
	reg86.bx = SET_LINEAR_MODE | mode;

	if(sys_int86(&reg86) != OK) {
		printf("vg_exit(): sys_int86() failed \n");
		return 1;
	}

	if(reg86.ah != 0)
		return 1;

	return 0;
}

